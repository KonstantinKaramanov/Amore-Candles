{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-undef": "off",
      "quotes": ["error", "double", { "allowTemplateLiterals": true }],
      "prefer-arrow-callback": "error"
    }
  }
  